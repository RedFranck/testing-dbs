{
    "forms" : [
        {
            "_id": "001",
            "form-name": "JCVD birthday party registration",
            "form-theme": "regular",
            "submit-url": "https://test.com/submit/form",
            "form-fields": [
                {
                    "field-name": "first-name",
                    "type": "text",
                    "label": "First Name",
                    "rules": [
                        {
                            "non-empty": true
                        }, {
                            "max-length": "40"
                        }
                    ]
                }, {
                    "field-name": "last-name",
                    "type": "text",
                    "label": "Last Name",
                    "rules": [
                        {
                            "max-length": "40"
                        }
                    ]
                }, {
                    "field-name": "email-address",
                    "type": "email",
                    "label": "Email Address",
                    "rules": [
                        {
                            "max-length": "100"
                        }
                    ]
                }, {
                    "field-name": "favorite-actor",
                    "type": "select",
                    "label": "Your favorite actor is :",
                    "rules": [
                        {
                            "value": "jcvd",
                            "error-message": "This is NOT the best actor, just think about it"
                        }
                    ],
                    "options": [
                        {
                            "option-value": "jcvd",
                            "option-label": "Jean-Claude Van Damme"
                        }, {
                            "option-value": "nic-cage",
                            "option-label": "Nicholas Cage"
                        }, {
                            "option-value": "al-pac",
                            "option-label": "Al Pacino"
                        }, {
                            "option-value": "arny",
                            "option-label": "Arnold Schwarzenegger"
                        }
                    ]
                }, {
                    "field-name": "preffered-day-week",
                    "type": "checkbox",
                    "label": "On what day of the week are you more likely to be available for the party ?",
                    "rules": [
                        {
                            "non-empty": true,
                            "error-message": "C'mon, select at least one option"
                        }
                    ],
                    "options": [
                        {
                            "option-value": "0",
                            "option-label": "Moon's Day"
                        }, {
                            "option-value": "1",
                            "option-label": "Tyr's Day"
                        }, {
                            "option-value": "2",
                            "option-label": "Oden's Day"
                        }, {
                            "option-value": "3",
                            "option-label": "Thor's Day"
                        }, {
                            "option-value": "4",
                            "option-label": "Frey's Day"
                        }, {
                            "option-value": "5",
                            "option-label": "Saturn's Day"
                        }, {
                            "option-value": "6",
                            "option-label": "Sun's Day"
                        }
                    ]
                }, {
                    "field-name": "dressing-code",
                    "type": "radio",
                    "label": "What will be your dressing code ?",
                    "rules": [
                        {
                            "non-empty": true
                        }, {
                            "non-value": "barbaric",
                            "error-message": "C'mon, it is a civilized event, ramp up your game a bit !"
                        }
                    ],
                    "options": [
                        {
                            "option-value": "class",
                            "option-label": "Top class"
                        }, {
                            "option-value": "cocktail",
                            "option-label": "Cocktail Apparel"
                        }, {
                            "option-value": "casual",
                            "option-label": "Casual"
                        }, {
                            "option-value": "barbaric",
                            "option-label": "As a caveman with wearing a sabertooth pelt"
                        }
                    ]
                }
            ]
        }, {
            "_id": "002",
            "form-name": "Nic Cage birthday party registration",
            "form-theme": "nic-cage",
            "submit-url": "https://test.com/submit/form",
            "form-fields": [
                {
                    "field-name": "first-name",
                    "type": "text",
                    "label": "First Name",
                    "rules": [
                        {
                            "non-empty": true
                        }, {
                            "max-length": "40"
                        }
                    ]
                }
            ]
        }
    ],

    "users" : [
        {
            "_id": "001",
            "name": "super user 0ne",
            "age": "66"
        }, {
            "_id": "002",
            "name": "super user tw0",
            "age": "66"
        }, {
            "_id": "003",
            "name": "super user tr0is",
            "age": "66"
        }
    ]
}